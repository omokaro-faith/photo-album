{
    "name": "photo-album",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.19.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "1.0.0",
        "nock": "^10.0.6",
        "prop-types": "^15.7.2",
        "raf": "3.3.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.1",
        "react-test-renderer": "^16.8.6",
        "redux": "^4.0.1",
        "redux-mock-store": "^1.5.3",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "coveralls": "^3.0.4",
        "react-scripts": "3.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "coverage": "react-scripts test --env=jsdom --coverage",
        "homepage": "http://myname.github.io/photo-album",
        "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!/node_modules/",
            "!src/index.js",
            "!src/serviceWorker.js",
            "!src/routes/AppRoutes.js",
            "!src/store/configureStore.js",
            "!src/tests/fixtures/albums.js",
            "!src/tests/fixtures/photos.js",
            "!src/tests/fixtures/users.js"
        ]
    }
}